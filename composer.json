{
  "authors": [
    {
      "name": "NNTmux Team"
    }
  ],
  "autoload": {
    "psr-4": {
      "nntmux\\": "nntmux/"
    },
    "classmap": [
      "www/pages/",
      "libs/pixelead0/Tmdb/",
      "libs/PEAR/Net_NNTP/NNTP/",
      "libs/"
    ]
  },
  "config": {
    "process-timeout": 3600,
    "preferred-install": {
      "*": "source"
    },
    "vendor-dir": "app/libraries"
  },
  "description": "A Usenet Indexer",
  "extra": {
    "branch-alias": {
      "dev-dev": "0.x-dev"
    },
    "asset-installer-paths": {
      "npm-asset-library": "www/themes/shared/assets",
      "bower-asset-library": "www/themes/shared/assets"
    }
  },
  "keywords": [
    "NNTmux",
    "usenet",
    "indexer",
    "lithium",
    "li3"
  ],
  "license": "GPL-3.0",
  "name": "nntmux/newznab-tmux",
  "non-feature-branches": [
    "latest-.*"
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/kbjr/Git.php"
    },
    {
      "type": "vcs",
      "url": "https://github.com/niel/li3_docs"
    },
    {
      "type": "vcs",
      "url": "https://github.com/niel/li3_quality"
    },
    {
      "type": "vcs",
      "url": "https://github.com/unionofrad/manual"
    }
  ],
  "require": {
    "php": ">=5.6.9",
    "barracudanetworks/forkdaemon-php": "~1.0",
    "dariusiii/rarinfo": "dev-master",
    "ext-ctype": "*",
    "ext-curl": "*",
    "ext-exif": "*",
    "ext-filter": "*",
    "ext-gd": "*",
    "ext-hash": "*",
    "ext-iconv": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-mcrypt": "*",
    "ext-mysqlnd": "*",
    "ext-openssl": "*",
    "ext-pcre": "*",
    "ext-pdo": "*",
    "ext-pdo_mysql": "*",
    "ext-session": "*",
    "ext-simplexml": "*",
    "ext-sockets": "*",
    "ext-spl": "*",
    "ext-xmlwriter": "*",
    "ext-zlib": "*",
    "fxp/composer-asset-plugin": "~1.1",
    "giantbomb/giantbomb": "@dev",
    "google/recaptcha": "~1.1",
    "james-heinrich/getid3": "1.9.*",
    "joshpinkney/tv-maze-php-api": "dev-master",
    "kbjr/Git.php": "dev-master",
    "moinax/tvdb": "~1.0",
    "phpmailer/phpmailer": "~5.2",
    "exeu/apai-io": "~1.0",
    "smarty/smarty": "~3.1",
    "bower-asset/animate.css": "~3.5.1",
    "bower-asset/autosize": "~3.0.15",
    "bower-asset/bootstrap-3.x": "~3.3.6",
    "bower-asset/bootstrap-datepicker" : "~1.6.4",
    "bower-asset/bootstrap-hover-dropdown": "~2.2.1",
    "bower-asset/bootstrap-progressbar": "~0.9.0",
    "bower-asset/colorbox": "~1.6.3",
    "bower-asset/chosen": "~1.5.1",
    "bower-asset/DataTables": "~1.10.11",
    "bower-asset/fastclick": "~1.0.6",
    "bower-asset/font-awesome": "~4.5",
    "bower-asset/html5shiv": "~3.7.3",
    "bower-asset/icheck": "~1.0.2",
    "bower-asset/jquery-2.2.x": "~2.2.1",
    "bower-asset/jquery-3.1.x": "~3.1.0",
    "bower-asset/jquery.nicescroll": "~3.6.8",
    "bower-asset/jquery-migrate-1.4.x": "~1.4.0",
    "bower-asset/jquery-migrate-3.0.x": "~3.0.0",
    "bower-asset/pace": "~1.0.2",
    "bower-asset/pnotify": "~3.0",
    "bower-asset/respond": "~1.4.2",
    "bower-asset/responsive-tables-js": "~1.0.6",
    "bower-asset/select2": "~4.0.2",
    "bower-asset/slimScroll": "~1.3.7",
    "bower-asset/tinymce-builded": "~4.4.3",
    "unionofrad/lithium": "^1.1-dev"
  },

  "require-dev": {
    "unionofrad/manual": "1.1.x-dev"
  },

  "scripts": {
  },
  "suggest": {
    "ext-simple_php_yenc_decode": "Greatly improves yEnc decoding speed in PHP 5.x",
    "ext-xdebug": "For developement work",
    "ext-yenc": "PHP7 and PHP5 yEnc decoding extension"
  },
  "type": "project"
}
